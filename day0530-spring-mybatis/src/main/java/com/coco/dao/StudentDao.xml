<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--
namespace：必须有值，自定义的唯一字符串 

          推荐使用：dao 接口的全限定名称 
-->
<mapper namespace="com.coco.dao.StudentDao">
    <insert id="insertStudent">
        insert into tstu values (#{id},#{name},#{email},#{age})
    </insert>
    <select id="selectStudents" resultType="com.coco.domain.Student">
        select id,name,email,age from tstu order by age desc
    </select>
</mapper>
